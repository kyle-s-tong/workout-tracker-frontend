{{#swiper-container class="w-full"}}
  {{#each this.model.workout.exercises as |exercise|}}
    {{#swiper-slide}}
      <div class="w-full">
        {{exercise.reps}}
      </div>
    {{/swiper-slide}}
  {{/each}}
{{/swiper-container}}


<div class="flex flex-row w-full h-16 fixed bottom">
  {{footer-button buttonNumber=1 buttonText="Cancel" click=(action "cancel") class="w-1/2 flex-grow"}}
  {{footer-button buttonNumber=1 buttonText="Finish workout" click=(action "finishWorkout") class="w-1/2 flex-grow"}}
</div>
